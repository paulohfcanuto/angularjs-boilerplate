{
  "name": "angularjs-boilerplate",
  "version": "0.0.1",
  "description": "A AngularJS boilerplate written in ES6",
  "license": "MIT",
  "author": "Paulo Henrique Fernandes Canuto <paulohfcanuto>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulohfcanuto/angularjs-boilerplate.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "build": "webpack --env=dev --progress",
    "build:prod": "webpack --env=prod --progress",
    "lint": "eslint src/**/*.js -c ./config/.eslintrc --ignore-path ./config/.eslintignore",
    "start": "webpack-dev-server --env=dev --open --progress --no-info",
    "start:prod": "webpack-dev-server --env=prod --open --progress --no-info",
    "test": "karma start ./config/karma.conf.js"
  },
  "keywords": [
    "AngularJS",
    "es6",
    "boilerplate",
    "webpack",
    "karma",
    "eslint",
    "babel"
  ],
  "dependencies": {
    "@uirouter/angularjs": "^1.0.11",
    "angular": "^1.7.9",
    "angular-mocks": "^1.6.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.6",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-chunk-hash": "^0.5.0",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "^4.1.1"
  }
}
